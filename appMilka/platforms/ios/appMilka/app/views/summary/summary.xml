
<Page navigatingTo="onLoad" class="page">
    <Page.actionBar>
        <ActionBar title="Assessment" icon="" class="action-bar">
          <ActionBar.actionItems>

          </ActionBar.actionItems>
        </ActionBar>
    </Page.actionBar>
<ScrollView>
  <StackLayout class="p-10">
    <Label class="h2" text="Graphs of registered data"></Label>

    <StackLayout class="graphContainer">
      <Label class="h2" text="EMR"></Label>
      <Label class="body" text="The first five questions from the questionnaire"></Label>
      <StackLayout id="barGraph">
      </StackLayout>
    </StackLayout>

    <StackLayout class="graphContainer">
      <Label class="h2" text="Paper"></Label>
      <Label class="body" text="The last five questions from the questionnaire"></Label>
      <StackLayout id="barGraphPaper">
      </StackLayout>
    </StackLayout>

    <StackLayout class="textStartContainer">

      <Label class="h2" text="Overview of study subjects"></Label>
      <Label class="body" text="Information of all study subjects"></Label>
    </StackLayout>
      <StackLayout id="informant" style="height: 500px;">
      </StackLayout>

    <StackLayout class="textStartContainer">
      <Label class="h2" text="Total overview"></Label>
      <Label class="body" text="All the data registered"></Label>
    </StackLayout>
      <StackLayout id="text">
      </StackLayout>



</StackLayout>
</ScrollView>
</Page>
